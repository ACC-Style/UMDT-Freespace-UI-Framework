            //- billboard - content(editable)            
            include /template/components/_billboard.pug
            
            //- breadcrumbs
            include /template/components/_breadcrumb.pug 
                //- breadcrumbs - content(editable)
                block contentBreadcrumb
                    nav(aria-label='You are here:', role='navigation')
                        ul.breadcrumbs--nav-list.breadcrumbs
                            li.breadcrumbs--nav-item
                                a(href='#').breadcrumbs--nav-link
                                    span.icon
                                        i.material-icons.md-18 folder_special
                                    span.text
                                        | project
                            li.breadcrumbs--nav-item
                                a(href='#').breadcrumbs--nav-link 
                                    span.icon
                                        i.material-icons.md-18 save
                                    span.text
                                        | CDD!
                            li.breadcrumbs--nav-item
                                a(href='#').breadcrumbs--nav-link 
                                    span.icon
                                        i.material-icons.md-18 lightbulb_outline
                                    span.text
                                        | concept
                            li.breadcrumbs--nav-item
                                    span.icon
                                        i.material-icons.md-18 change_history
                                    span.text
                                        | data element
                            li.breadcrumbs--nav-item  
                                    span.icon
                                        i.material-icons.md-18 mode_edit
                                    span.text
                                        | element*

