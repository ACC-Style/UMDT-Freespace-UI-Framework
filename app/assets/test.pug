doctype html
html.no-js(lang='en')

    //- head
    include /template/layout/_head.pug

    body
        //- off canvas menus
        include /template/modules/_off.canvas.left.reports.pug 
        include /template/modules/_off.canvas.left.filter.pug 
        include /template/modules/_off.canvas.left.search.pug 
        include /template/modules/_off.canvas.right.updates.pug 
        include /template/modules/_off.canvas.right.comments.pug 
        include /template/modules/_off.canvas.right.references.pug 
        //- include /template/modules/_off.canvas.left.pug 
        //- include /template/modules/_off.canvas.right.pug

        //- page body
