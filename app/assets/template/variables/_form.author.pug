form(data-abide='7v3wyo-abide', novalidate='')
    .grid-x.grid-padding-x
        .cell.small-4
            .form--input-text.-is-required
                label
                    | Major Categories
                input(type='text', placeholder='1234', readonly, aria-describedby='exampleHelpText', required='', pattern='number', value="1212")
                span.form-error
                    | A name of the new create is required.
                //- p#exampleHelpText.help-text.-has-error Any help information (what is resticted or allow with name input)
        .cell.small-4

            .form--input-text.-is-required
                label
                    | Minor Categories
                input(type='text', placeholder='1234', aria-describedby='exampleHelpText', required='', pattern='number', value="1212")
                span.form-error
                    | A name of the new create is required.
                //- p#exampleHelpText.help-text.-has-error Any help information (what is restricted or allow with name input)
        .cell.small-2

                    .form--input-text
                        .switch.tiny.switch-disable
                                label.switch-value(for='swtich-active-s')
                                    | Supressed
                                input#swtich-active-s.switch-input(type='checkbox', name='exampleSwitch')
                                label.switch-paddle(for='swtich-active-s')
                                    span.show-for-sr Edit Mode
                                    span.switch-active(aria-hidden='true')
                                        i.material-icons.md-16(aria-hidden='true') visibility
                                    span.switch-inactive(aria-hidden='true')
                                        i.material-icons.md-16(aria-hidden='true') visibility_off
        .cell.small-2
            .form--input-text.-is-required
                label
                    | ID
                input(type='text', placeholder='1234', aria-describedby='exampleHelpText', required='', pattern='number', value="1212")
