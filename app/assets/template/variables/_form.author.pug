                        //- p#exampleHelpText.help-text.-has-error Any help information (what is restricted or allow with name input)

        .cell.small-12
            .grid-y
                .cell
                    .form--input-text.-is-required
                        label
                            | Line Text
                        input(type='text', placeholder='phrase', aria-describedby='exampleHelpText', value="Matt")
                        //- p#help2.help-text
                            | {Month}{date}{year}{timePM}
        .cell.small-12
            .grid-y
                .cell
                    .form--input-text.-is-required
                        label
                            | Line Text
                        input(type='text', placeholder='phrase', aria-describedby='exampleHelpText', value="Matt")
                        //- p#help2.help-text
                            | {Month}{date}{year}{timePM}

        .cell.divider-section
